n = int(input("enter a number: "))

if(n==0 or n==1):
    print("not a prime number")
    
elif(n==2):
    print("prime number")
    
else:
    is_prime = True
    for i in range(2,n):
        if(n%i==0):
            is_prime = False
            break;
        
        else:
            is_prime = True
    print("prime" if(is_prime == True) else "not a prime")
